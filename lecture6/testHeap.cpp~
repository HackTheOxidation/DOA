#include "MinHeap.hpp"
#include <iostream>
#include <string>

using namespace std;

int main(void) {
  int testData[6] = {6, 5, 4, 3, 2, 1};

  cout << "---MinHeap Test---\nTest data: ";
  for (int i = 0; i < 6; i++) {
    cout << testData[i] << " ";
  }
  cout << endl;

  MinHeap<int> heap(testData, 6);

  cout << "After heapification: ";
  heap.print();
  cout << endl;

  return 0;
}
    
